						},
						"parameters": {}
					},
					{
						"pipelineReference": {
							"referenceName": "5 recrea ine",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 1,
						"startTime": "2023-10-19T08:11:00",
						"endTime": "2023-10-26T17:11:00",
						"timeZone": "Romance Standard Time",
